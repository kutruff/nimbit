{
  "name": "@kutruff-planner/db",
  "version": "0.1.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:generate": "pnpm with-env prisma generate",
    "db:push": "pnpm with-env prisma db push --skip-generate",
    "db:push-reset": "pnpm with-env prisma db push --force-reset && pnpm db:seed",
    "db:migrate-dev": "pnpm with-env prisma migrate dev",
    "db:migrate": "pnpm with-env prisma migrate deploy",
    "db:seed": "pnpm with-env prisma db seed",
    "db:reset": "pnpm with-env prisma migrate reset",
    "disabled-dev": "pnpm with-env prisma studio --port 5556",
    "with-env": "dotenv -e ../../.env --"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "prisma": "^4.12.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4"
  }
}
